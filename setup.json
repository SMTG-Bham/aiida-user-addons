{
    "name": "aiida-user-addons",
    "author": "Bonan Zhu",
    "author_email": "zhubonan@outlook.com",
    "description": "User workflow plugin",
    "license": "Creative Commons",
    "classifiers": [
        "Programming Language :: Python"
    ],
    "version": "0.1.0",
    "install_requires": [
        "aiida-vasp>=2.0",
        "aiida-core>=1.1.0",
        "bsym==1.1.1"
    ],
    "entry_points": {
        "console_scripts":
        [
            "vasp-dryrun=aiida_user_addons.cmdline.cmd_vasp_dryrun:cmd_vasp_dryrun"
        ],
        "aiida.schedulers": [
            "pbsarcher = aiida_user_addons.schedulers.pbsarcher:PbsArcherScheduler",
            "sgenodetail = aiida_user_addons.schedulers.sgenodetail:SgeNoDetailScheduler"
        ],
        "aiida.workflows": [
            "vaspu.relax = aiida_user_addons.vworkflows.relax:VaspRelaxWorkChain",
            "vaspu.converge = aiida_user_addons.vworkflows.converge:RapidConvergeWorkChain",
            "vaspu.vasp = aiida_user_addons.vworkflows.vasp:VaspWorkChain",
            "vaspu.bands = aiida_user_addons.vworkflows.new_bands:VaspBandsWorkChain",
            "vaspu.master = aiida_user_addons.vworkflows.master:MasterWorkChain",
            "vaspu.phonopy = aiida_user_addons.vworkflows.phonopy:VaspAutoPhononWorkChain",
            "vaspu.magnetic = aiida_user_addons.vworkflows.magnetic:SpinEnumerateWorkChain",
            "vaspu.delithiate = aiida_user_addons.vworkflows.delithiate:SimpleDelithiateWorkChain"
        ],
        "aiida.cmdline.data": [
            "addons = aiida_user_addons.cmdline.cmd_addons:addons"
        ]
    },
    "extras_require": {
        "testing": [
            "aiida-core[testing]"
        ],
        "pre-commit": [
            "pre-commit==1.18.3",
            "yapf==0.28.0",
            "prospector==1.2.0",
            "pylint==2.4.4"
        ],
        "docs": [
            "sphinx"
        ]
    }
}
